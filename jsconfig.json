{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~components/*": [
        "src/components/*"
      ],
      "~config": [
        "site.config"
      ],
      "~data/*": [
        "src/data/*"
      ],
      "~images/*": [
        "src/images/*"
      ],
      "~layouts/*": [
        "src/layouts/*"
      ],
      "~styles/*": [
        "src/styles/*"
      ],
      "~theme": [
        "src/theme"
      ]
    }
  }
}
